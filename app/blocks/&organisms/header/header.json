{
  "header": {
    "logo": {
      "type": "header",
      "class": "main-header__logo inert-element",
      "attrs": "aria-label='Главная'"
    },
    "meta_nav": {
      "class": "main-header__meta-nav",
      "list": {
        "items": [{
          "button": {
            "class": "meta-nav__btn search-btn inert-element",
            "icon": {
              "class": "meta-nav__btn-icon",
              "name": "sprite-multi.svg#magnifying-glass"
            },
            "attrs": "aria-label='Поиск'"
          }
        }, {
          "button": {
            "class": "meta-nav__btn inert-element",
            "href": "#",
            "icon": {
              "class": "meta-nav__btn-icon",
              "name": "sprite-multi.svg#heart"
            },
            "attrs": "aria-label='Избранное'"
          }
        }, {
          "button": {
            "basket": true,
            "class": "meta-nav__btn drawer-btn inert-element",
            "icon": {
              "class": "meta-nav__btn-icon",
              "name": "sprite-multi.svg#cart-shopping"
            },
            "text": {
              "class": "meta-nav__btn-num",
              "text": 2
            },
            "attrs": "aria-label='Корзина'"
          }
        }, {
          "button": {
            "class": "meta-nav__btn inert-element",
            "icon": {
              "class": "meta-nav__btn-icon",
              "name": "sprite-multi.svg#user"
            },
            "attrs": "aria-label='Профиль' data-fancybox data-src='#popup_auth_and_registration'"
          }
        }]
      }
    }
  }
}