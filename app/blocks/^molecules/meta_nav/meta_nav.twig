<nav class="meta-nav {{ meta_nav.class }}" aria-label="Мета">
  <ul class="meta-nav__list">
    {% for item in meta_nav.list.items %}
      <li class="meta-nav__item">
        {% view "@button" with {
          button: item.button|merge({
            href: "#",
            class: "meta-nav__btn" ~ (item.button.class is defined ? " " ~ item.button.class : "")
          })
        } %}
      </li>
    {% endfor %}
  </ul>
</nav>